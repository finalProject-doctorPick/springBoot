package com.example.persistence;

import org.springframework.data.repository.CrudRepository;

import com.example.domain.Employee;

public interface EmployeeRepository extends CrudRepository<Employee, Integer> {
	// 기본함수
	// 전체검색 	: findAll()
	// 아이디검색 	: findById()
	// 입력		: save()
	// 삭제		: deleteById()
	// 상세 검색	: findBy컬럼명,키워드()
}
